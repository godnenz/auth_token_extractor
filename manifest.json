{
  "manifest_version": 3,
  "name": "Bythen Auth Token Extractor",
  "version": "1.0",
  "description": "Extracts the BYTHEN_AUTH access token from cookies.",
  "permissions": [
    "cookies",
    "tabs",
    "clipboardWrite"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
